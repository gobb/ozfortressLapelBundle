<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                        xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                        http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="ozfortress\LapelBundle\Entity\UserRank" table="ozfortress_lapel_userrank">

        <id name="id" column="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field name="class" column="class" type="string" />

        <field name="identifier" column="identifier" type="string" />

        <field name="index" column="rankIndex" type="integer" />

        <field name="providerAlias" column="providerAlias" type="string" />

    </entity>

</doctrine-mapping>
